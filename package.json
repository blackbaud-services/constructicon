{
  "name": "constructicon",
  "version": "4.1.3",
  "description": "Library of re-usable components for Professional Services projects",
  "main": "index.js",
  "scripts": {
    "start": "styleguidist server",
    "lint": "prettier-standard --check 'source/**/*.js'",
    "test": "mocha source/**/**/__tests__/*.js --recursive --ui bdd --require @babel/register --require test/common.js",
    "format": "prettier-standard 'source/**/*.js'",
    "precommit": "lint-staged",
    "build": "babel source -d dist --copy-files",
    "build:styleguide": "styleguidist build",
    "build:icons": "svgo -f ./icons; tail -n +1 ./icons/*.svg > ./icons.xml",
    "deploy:prepare": "npm run build && gulp npm-prepare",
    "deploy:docs": "npm run build:styleguide && gh-pages -d styleguide",
    "announce": "./bin/announce"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blackbaud-services/constructicon.git"
  },
  "bugs": {
    "url": "https://github.com/blackbaud-services/constructicon/issues"
  },
  "author": "Dan Henderson <dan.henderson@blackbaud.com>",
  "license": "MIT",
  "dependencies": {
    "@emotion/css": "^11.1.3",
    "@emotion/server": "^11.4.0",
    "axios": "^0.21.4",
    "dayjs": "^1.10.7",
    "lodash": "^4.17.21",
    "prop-types": "^15.7.2",
    "query-string": "7.0.1",
    "react-avatar-editor": "^12.0.0",
    "react-contenteditable": "^3.3.6",
    "react-dropzone": "^11.3.5",
    "react-modal": "^3.14.3",
    "react-onclickoutside": "^6.11.2",
    "react-slick": "^0.28.1",
    "react-slider": "^1.3.1",
    "react-ticker": "^1.2.2",
    "xss": "^1.0.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/register": "^7.15.3",
    "babel-loader": "^8.2.2",
    "chai": "^4.3.4",
    "css-loader": "^5.1.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.32.0",
    "gh-pages": "^3.2.3",
    "gulp": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.3",
    "husky": "^7.0.2",
    "jsdom": "^17.0.0",
    "lint-staged": "^11.1.2",
    "lodash": "^4.17.21",
    "mocha": "^9.1.1",
    "moxios": "^0.4.0",
    "prettier": "^2.4.0",
    "prettier-standard": "^16.4.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^5.2.0",
    "react-styleguidist": "^11.1.7",
    "sinon": "^11.1.2",
    "source-map-loader": "^1.0.0",
    "style-loader": "^2.0.0",
    "svgo": "^2.5.0",
    "webpack": "^4.44.2"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0",
    "react-dom": "^16.8.0 || ^17.0.0",
    "react-helmet": "^5.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "standard": {
    "ignore": [
      "source/**/**/__tests__/*.js",
      "test",
      "styleguide.config.js",
      "gulpfile.js"
    ]
  },
  "lint-staged": {
    "source/**/*.js": "prettier-standard"
  }
}
