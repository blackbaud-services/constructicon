{
  "name": "constructicon",
  "version": "2.7.1",
  "description": "Library of re-usable components for Professional Services projects",
  "main": "index.js",
  "scripts": {
    "start": "styleguidist server",
    "lint": "standard",
    "test": "mocha source/**/**/__tests__/*.js",
    "format": "prettier-standard 'source/**/*.js'",
    "precommit": "lint-staged",
    "build": "babel source -d dist --copy-files",
    "build:styleguide": "styleguidist build",
    "deploy:styleguide": "npm run build:styleguide && gh-pages -d styleguide",
    "deploy:prepare": "gulp npm-prepare"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/everydayhero/constructicon.git"
  },
  "bugs": {
    "url": "https://github.com/everydayhero/constructicon/issues"
  },
  "author": "Dan Henderson <dan.henderson@everydayhero.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.2",
    "emotion": "^9.2.7",
    "emotion-server": "^9.2.7",
    "filestack-js": "^3.12.4",
    "lodash": "^4.17.15",
    "moment": "^2.26.0",
    "prop-types": "^15.7.2",
    "query-string": "5.1.1",
    "react-avatar-editor": "^11.0.9",
    "react-contenteditable": "^3.3.4",
    "react-dropzone": "^11.0.1",
    "react-modal": "^3.2.1",
    "react-onclickoutside": "^6.9.0",
    "react-slick": "^0.26.1",
    "react-slider": "^1.0.7",
    "sanitize-html": "^1.24.0"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^5.6.0",
    "gh-pages": "^0.12.0",
    "gulp": "^3.9.1",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "husky": "^0.14.3",
    "jsdom": "^9.9.1",
    "lint-staged": "^7.2.2",
    "mocha": "^2.5.3",
    "prettier": "^1.14.3",
    "prettier-standard": "^8.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.0",
    "react-styleguidist": "^6.2.6",
    "sinon": "^4.1.3",
    "standard": "^10.0.2",
    "webpack": "2.2.0"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0",
    "react-helmet": "^5.0.0"
  },
  "standard": {
    "ignore": [
      "source/**/**/__tests__/*.js",
      "test",
      "gulpfile.js"
    ],
    "globals": [
      "it",
      "describe",
      "mount",
      "expect",
      "utils"
    ]
  },
  "lint-staged": {
    "linters": {
      "source/**/*.js": [
        "prettier-standard",
        "git add"
      ]
    }
  }
}
